
{
  "name": "winforms-designer",
  "displayName": "WinForms Designer fun",
  "description": "Visual designer for Windows Forms with two-way editing",
  "version": "1.12.10",
    "publisher": "atkivisolutioninstadotnetmodelgenerator",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Visualization",
    "Designers"
  ],
   "icon": "image/logo.png",
  "activationEvents": [
    "onCustomEditor:winformsDesigner.designer"
  ],
  "main": "./extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "winformsDesigner.designer",
        "displayName": "WinForms Designer",
        "selector": [
          {
            "filenamePattern": "*.Designer.cs"
          }
        ]
      }
    ],
     "keywords": [
    "dotnet8",
    "winform",
    "winform designer",
    "dark mode",
    "drag and drop",
    "vs code winform designer",
    "c# winform designer"
    
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/spring-boot-scaffold-generator"
  },
  "bugs": {
    "url": "https://github.com/yourusername/spring-boot-scaffold-generator/issues"
  },
  "homepage": "https://github.com/yourusername/spring-boot-scaffold-generator",
  "license": "MIT",
    "commands": [
      {
        "command": "winformsDesigner.openDesigner",
        "title": "Open with WinForms Designer",
        "category": "WinForms"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "winformsDesigner.openDesigner",
          "when": "resourceLangId == csharp"
        }
      ],
      "explorer/context": [
        {
          "command": "winformsDesigner.openDesigner",
          "when": "resourceFilename =~ /(\\.Designer\\.cs)$/",
          "group": "navigation"
        }
      ]
    }
  }
}
